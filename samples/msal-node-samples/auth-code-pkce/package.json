  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^16.10.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "workspaces": [
    "../../../lib/msal-node"
  ]
